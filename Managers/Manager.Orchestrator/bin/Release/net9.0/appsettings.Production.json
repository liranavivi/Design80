{
    "ConnectionStrings": {
        "MongoDB": "${MONGODB_CONNECTION_STRING:mongodb://localhost:27017}"
    },
    "MassTransit": {
        "Provider": "${MASSTRANSIT_PROVIDER:RabbitMQ}"
    },
    "RabbitMQ": {
        "Host": "${RABBITMQ_HOST:localhost}",
        "Username": "${RABBITMQ_USERNAME:guest}",
        "Password": "${RABBITMQ_PASSWORD:guest}"
    },
    "Kafka": {
        "BootstrapServers": "${KAFKA_BOOTSTRAP_SERVERS:localhost:9092}"
    },
    "Hazelcast": {
        "ClusterName": "${HAZELCAST_CLUSTER_NAME:EntitiesManager}",
        "NetworkConfig": {
            "Addresses": [
                "${HAZELCAST_ADDRESS:127.0.0.1:5701}"
            ]
        }
    },
    "OpenTelemetry": {
        "Endpoint": "${OTEL_EXPORTER_OTLP_ENDPOINT:http://localhost:4317}",
        "HealthEndpoint": "${OTEL_HEALTH_ENDPOINT:http://localhost:8081}"
    },
    "ManagerUrls": {
        "Step": "${STEP_MANAGER_URL:http://localhost:5170}",
        "Assignment": "${ASSIGNMENT_MANAGER_URL:http://localhost:5130}",
        "Address": "${ADDRESS_MANAGER_URL:http://localhost:5120}",
        "Delivery": "${DELIVERY_MANAGER_URL:http://localhost:5150}",
        "Plugin": "${PLUGIN_MANAGER_URL:http://localhost:5190}",
        "Schema": "${SCHEMA_MANAGER_URL:http://localhost:5160}",
        "OrchestratedFlow": "${ORCHESTRATED_FLOW_MANAGER_URL:http://localhost:5140}",
        "Workflow": "${WORKFLOW_MANAGER_URL:http://localhost:5180}"
    },
    "ProcessorHealthMonitor": {
        "MapName": "${PROCESSOR_HEALTH_MAP_NAME:processor-health}"
    },
    "OrchestrationCache": {
        "MapName": "${ORCHESTRATION_CACHE_MAP_NAME:orchestration-data}"
    },
    "ProcessorActivityDataCache": {
        "MapName": "${PROCESSOR_ACTIVITY_MAP_NAME:processor-activity}"
    }
}
