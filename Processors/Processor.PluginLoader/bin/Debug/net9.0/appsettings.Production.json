{
  "MassTransit": {
    "Provider": "${MASSTRANSIT_PROVIDER:RabbitMQ}"
  },
  "RabbitMQ": {
    "Host": "${RABBITMQ_HOST:localhost}",
    "Username": "${RABBITMQ_USERNAME:guest}",
    "Password": "${RABBITMQ_PASSWORD:guest}"
  },
  "Kafka": {
    "BootstrapServers": "${KAFKA_BOOTSTRAP_SERVERS:localhost:9092}"
  },
  "Hazelcast": {
    "ClusterName": "${HAZELCAST_CLUSTER_NAME:EntitiesManager}",
    "NetworkConfig": {
      "Addresses": [
        "${HAZELCAST_ADDRESS:127.0.0.1:5701}"
      ]
    }
  },
  "OpenTelemetry": {
    "Endpoint": "${OTEL_EXPORTER_OTLP_ENDPOINT:http://localhost:4317}",
  },
  "ProcessorHealthMonitor": {
    "MapName": "${PROCESSOR_HEALTH_MAP_NAME:processor-health}"
  },
  "ProcessorActivityDataCache": {
    "MapName": "${PROCESSOR_ACTIVITY_MAP_NAME:processor-activity}"
  }
}
